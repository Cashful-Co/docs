{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Cashful API Documentation",
  "colors": {
    "primary": "#1825e8",
    "light": "#1825e8",
    "dark": "#1825e8"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "/api-reference/introduction"
            ]
          },
          {
            "group": "Health",
            "icon": "heart",
            "pages": [
              "/api-reference/endpoint/api/canary/health/health-check"
            ]
          },
          {
            "group": "Balance",
            "icon": "wallet",
            "pages": [
              "/api-reference/endpoint/api/canary/balance/get-merchant-balance"
            ]
          },
          {
            "group": "Checkout",
            "icon": "credit-card",
            "pages": [
              "/api-reference/endpoint/api/canary/checkout/sessions/create-hosted-checkout"
            ]
          },
          {
            "group": "Customers",
            "icon": "users",
            "pages": [
              "/api-reference/endpoint/api/canary/customers/list-customers",
              "/api-reference/endpoint/api/canary/customers/create-customer",
              "/api-reference/endpoint/api/canary/customers/id/retrieve-customer",
              "/api-reference/endpoint/api/canary/customers/id/update-customer",
              "/api-reference/endpoint/api/canary/customers/id/balance/get-customer-cash-balance",
              "/api-reference/endpoint/api/canary/customers/id/payment-methods/list-customer-payment-methods",
              "/api-reference/endpoint/api/canary/customers/id/transactions/list-customer-transactions"
            ]
          },
          {
            "group": "Events",
            "icon": "bell",
            "pages": [
              "/api-reference/endpoint/api/canary/events/list-events"
            ]
          },
          {
            "group": "Payment Intents",
            "icon": "zap",
            "pages": [
              "/api-reference/endpoint/api/canary/payment-intents/create-off-session-charge",
              "/api-reference/endpoint/api/canary/payment-intents/id/retrieve-payment-intent"
            ]
          },
          {
            "group": "Payment Methods",
            "icon": "credit-card",
            "pages": [
              "/api-reference/endpoint/api/canary/payment-methods/id/retrieve-payment-method",
              "/api-reference/endpoint/api/canary/payment-methods/id/delete-payment-method"
            ]
          },
          {
            "group": "Products",
            "icon": "box",
            "pages": [
              "/api-reference/endpoint/api/canary/products/list-products",
              "/api-reference/endpoint/api/canary/products/create-product",
              "/api-reference/endpoint/api/canary/products/id/update-product"
            ]
          },
          {
            "group": "Webhooks",
            "icon": "webhook",
            "pages": [
              "/api-reference/endpoint/api/canary/webhook-endpoints/list-webhook-endpoints",
              "/api-reference/endpoint/api/canary/webhook-endpoints/create-webhook-endpoint",
              "/api-reference/endpoint/api/canary/webhook-endpoints/id/delete-webhook-endpoint"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "fonts": {
    "heading": {
      "family": "Trade Gothic",
      "source": "/fonts/Trade-Gothic/Trade-Gothic-Std-Extended.otf",
      "format": "woff2",
      "weight": 700
    },
    "body": {
      "family": "Aeonik",
      "source": "/fonts/Aeonik/Aeonik-Regular.otf",
      "format": "woff2",
      "weight": 400
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@cashful.africa"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.cashful.africa"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  },
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": [
        "curl",
        "python",
        "javascript",
        "node"
      ],
      "defaults": "required",
      "prefill": true
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/cashful-co",
      "linkedin": "https://www.linkedin.com/company/cashful"
    }
  }
}