{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Cashful API Documentation",
  "colors": {
    "primary": "#1825e8",
    "light": "#1825e8",
    "dark": "#1825e8"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "/guides/getting-started/overview",
              "/guides/getting-started/authentication"
            ]
          }
        ]
      },
      {
        "tab": "Tutorials",
        "groups": [
          {
            "group": "Merchant Quick Start",
            "pages": [
              "/guides/tutorials/merchant-onboarding",
              "/guides/tutorials/create-a-checkout-session",
              "/guides/tutorials/manage-customers"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Authentication",
            "icon": "lock",
            "pages": [
              "/api-reference/endpoint/api/canary/authentication/sign-up/email/sign-up-email",
              "/api-reference/endpoint/api/canary/authentication/sign-in/email/sign-in-email",
              "/api-reference/endpoint/api/canary/authentication/sign-out/sign-out",
              "/api-reference/endpoint/api/canary/authentication/get-session/get-session",
              "/api-reference/endpoint/api/canary/authentication/forget-password/forget-password",
              "/api-reference/endpoint/api/canary/authentication/reset-password/reset-password"
            ]
          },
          {
            "group": "Organisations",
            "icon": "building",
            "pages": [
              "/api-reference/endpoint/api/canary/authentication/organization/create/create-organization",
              "/api-reference/endpoint/api/canary/authentication/organization/list/list-organizations",
              "/api-reference/endpoint/api/canary/authentication/organization/set-active/set-active-organization",
              "/api-reference/endpoint/api/canary/authentication/organization/update/update-organization",
              "/api-reference/endpoint/api/canary/authentication/organization/delete/delete-organization",
              "/api-reference/endpoint/api/canary/authentication/organization/remove-member/remove-member",
              "/api-reference/endpoint/api/canary/authentication/organization/update-member-role/update-member-role"
            ]
          },
          {
            "group": "API Keys",
            "icon": "key",
            "pages": [
              "/api-reference/endpoint/api/canary/authentication/api-key/create-api-key",
              "/api-reference/endpoint/api/canary/authentication/api-key/list-api-keys",
              "/api-reference/endpoint/api/canary/authentication/api-key/update-api-key",
              "/api-reference/endpoint/api/canary/authentication/api-key/delete-api-key",
              "/api-reference/endpoint/api/canary/authentication/api-key/verify-api-key"
            ]
          },
          {
            "group": "Balance",
            "icon": "wallet",
            "pages": [
              "/api-reference/endpoint/api/canary/balance/get-merchant-balance"
            ]
          },
          {
            "group": "Checkout",
            "icon": "shopping-cart",
            "pages": [
              "/api-reference/endpoint/api/canary/checkout/sessions/create-checkout-session"
            ]
          },
          {
            "group": "Payment Links",
            "icon": "link",
            "pages": [
              "/api-reference/endpoint/api/canary/payment-links/list-payment-links",
              "/api-reference/endpoint/api/canary/payment-links/create-payment-link",
              "/api-reference/endpoint/api/canary/payment-links/id/update-payment-link"
            ]
          },
          {
            "group": "Payment Methods",
            "icon": "credit-card",
            "pages": [
              "/api-reference/endpoint/api/canary/payment-methods/id/retrieve-payment-method",
              "/api-reference/endpoint/api/canary/payment-methods/id/delete-payment-method"
            ]
          },
          {
            "group": "Products",
            "icon": "box",
            "pages": [
              "/api-reference/endpoint/api/canary/products/list-products",
              "/api-reference/endpoint/api/canary/products/create-product",
              "/api-reference/endpoint/api/canary/products/id/update-product"
            ]
          },
          {
            "group": "Customers",
            "icon": "users",
            "pages": [
              "/api-reference/endpoint/api/canary/customers/list-customers",
              "/api-reference/endpoint/api/canary/customers/create-customer",
              "/api-reference/endpoint/api/canary/customers/id/retrieve-customer",
              "/api-reference/endpoint/api/canary/customers/id/update-customer",
              "/api-reference/endpoint/api/canary/customers/id/balance/get-customer-cash-balance",
              "/api-reference/endpoint/api/canary/customers/id/payment-methods/list-customer-payment-methods",
              "/api-reference/endpoint/api/canary/customers/id/transactions/list-customer-transactions"
            ]
          },
          {
            "group": "Webhooks",
            "icon": "webhook",
            "pages": [
              "/api-reference/endpoint/api/canary/webhook-endpoints/list-webhook-endpoints",
              "/api-reference/endpoint/api/canary/webhook-endpoints/create-webhook-endpoint",
              "/api-reference/endpoint/api/canary/webhook-endpoints/id/delete-webhook-endpoint"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "fonts": {
    "heading": {
      "family": "Trade Gothic",
      "source": "/fonts/Trade-Gothic/Trade-Gothic-Std-Extended.otf",
      "format": "woff2",
      "weight": 700
    },
    "body": {
      "family": "Aeonik",
      "source": "/fonts/Aeonik/Aeonik-Regular.otf",
      "format": "woff2",
      "weight": 400
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@cashful.africa"
      }
    ]
  },
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  },
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": [
        "curl",
        "python",
        "javascript",
        "node"
      ],
      "defaults": "required",
      "prefill": true
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/cashful-co",
      "linkedin": "https://www.linkedin.com/company/cashful"
    }
  }
}